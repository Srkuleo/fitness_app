generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Exercise {
  id     Int    @id @default(autoincrement())
  type   String
  name   String @unique
  sets   Int
  reps   String
  weight String
  rest   String
}

model Workout {
  id   Int    @id @default(autoincrement())
  name String @unique
}
